{
    "luci-app-singbox-ui": {
        "description": "Sing-box management access",
        "read": {
            "file": {
                "/etc/init.d/sing-box": [ "exec" ],
                "/etc/init.d/singbox-ui-autoupdater": [ "exec" ],
                "/etc/sing-box/*": ["read"],
                "/usr/bin/singbox-ui/*": ["exec"],
                "/tmp/*": ["read"]
            },
            "uci": ["singbox-ui"]
        },
        "write": {
            "file": {
                "/etc/sing-box/*": ["write"],
                "/tmp/*": ["write"]
            },
            "uci": ["singbox-ui"]
        }
    }
}
